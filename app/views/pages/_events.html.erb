<%= turbo_frame_tag :events_frame do %>
  <div class="mb-4">
    <p class="w-100">
      <%= link_to "Add New Event", new_event_path, data: {turbo_frame: "_top"}, class: "btn btn-secondary btn-lg w-100 link-underline link-underline-opacity-0 mb-3 mt-2" %>
    </p>
  </div>
  <% @events.each do |event| %>
    <%= render "special_card_myevent_page", event: event %>
    <%= render "special_followers_myevent_page", event: event %>
  <% end %>
<% end %>
